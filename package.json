{
  "name"        : "cleaner-node",
  "version"     : "1.0.18",
  "description" : "Helpful utilities and scripts to make Node projects more legible and easier for the next developer to take over.",
  "main"        : "src/index.js",
  "preferGlobal": true,
  "scripts"     : {
    "lint"    : "eslint ./src/**/*.js",
    "lint:fix": "eslint ./src/**/*.js --fix",
    "test"    : "jest"
  },
  "repository": {
    "type": "git",
    "url" : "git+https://github.com/FredLackey/cleaner-node.git"
  },
  "keywords": [
    "node",
    "nodejs",
    "clean",
    "clean",
    "code",
    "utils",
    "utility",
    "utilities",
    "lodash",
    "alternative",
    "underscore",
    "alternative"
  ],
  "author": {
    "name" : "Fred Lackey",
    "email": "fred.lackey@gmail.com",
    "url"  : "http://www.fredlackey.com"
  },
  "license": "Apache-2.0",
  "bugs"   : {
    "url": "https://github.com/FredLackey/cleaner-node/issues"
  },
  "homepage": "https://github.com/FredLackey/cleaner-node#readme",
  "jest"    : {
    "testEnvironment"           : "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "dependencies": {
    "jsonwebtoken": "^9.0.2",
    "node-fetch"  : "2.7.0",
    "rimraf"      : "5.0.5",
    "uuid"        : "^9.0.1"
  },
  "devDependencies": {
    "eslint"                   : "^8.56.0",
    "eslint-config-airbnb"     : "^19.0.4",
    "eslint-config-standard"   : "^17.1.0",
    "eslint-plugin-import"     : "^2.29.1",
    "eslint-plugin-jsx-a11y"   : "^6.8.0",
    "eslint-plugin-node"       : "^11.1.0",
    "eslint-plugin-promise"    : "^6.1.1",
    "eslint-plugin-react"      : "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-standard"   : "^4.1.0",
    "jest"                     : "^29.7.0"
  }
}
